{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["es5", "es6", "DOM.Iterable", "DOM"],
    "jsx": "react-jsx",
    "sourceMap":true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/*"],
  "exclude": ["node_modules", "dist"]
}